{
  "name": "quotation-apps-backend",
  "version": "1.0.0",
  "description": "Backend API for Quotation Apps",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "lint": "eslint .",
    "optimize": "node -e \"require('./utils/database').optimize()\"",
    "backup": "node -e \"require('./utils/database').backup('./backup/backup.db')\"",
    "stats": "node -e \"console.log(JSON.stringify(require('./utils/database').getStats(), null, 2))\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "sanitize-html": "^2.11.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.5"
  },
  "keywords": [
    "quotation",
    "api",
    "express",
    "sqlite",
    "nodejs"
  ],
  "author": "Quotation Apps Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
